            <string>///UPDATE UI OBJECTS

//////////////////////////////////////////////////////////////////
//  SELECTION
////
    var lclick_time = time_since_lclick,
        currently_selected = selected_item;
    with(objUI)
    {
        if(mouse_check_button_pressed(mb_left))
        {
            other.time_since_lclick = current_time;
            
            if(point_in_rectangle( mouse_x, mouse_y, x, y, (x + width), (y + height) ))
            {
                if(double_click &amp;&amp; !(id == currently_selected))
                {
                    if((current_time - lclick_time) &lt; 500)
                    {
                        other.selected_item = id;
                        break;
                    }
                }
                else
                {
                    other.selected_item = id;
                    break;
                }                    
            }
            else
            {
                other.selected_item = noone;
            }
        }
    }


//////////////////////////////////////////////////////////////////
//  ACTIVATE
////
    if(selected_item != prev_selected_item)
    {
        with(prev_selected_item)
            event_user(UI_DEACTIVATE);
        
        with(selected_item)
            event_user(UI_ACTIVATE);
            
        prev_selected_item = selected_item;
    }


//////////////////////////////////////////////////////////////////
//  HOVER
////
    

//////////////////////////////////////////////////////////////////
//  LEFT MOUSE BUTTON
////
    // CLICK
    if(mouse_check_button_pressed(mb_left))
    {
        with(selected_item)
            event_user(UI_LCLICK);
    }
    
    // HOLD
    if(mouse_check_button(mb_left) &amp;&amp; !mouse_check_button_pressed(mb_left))
    {
        with(selected_item)
            event_user(UI_LHOLD);
    }
    
    // RELEASE
    if(mouse_check_button_released(mb_left))
    {
        with(selected_item)
            event_user(UI_LRELEASE);
    }


</string>
